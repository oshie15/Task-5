{
	"name": "bookstore-test-app",
	"version": "1.0.0",
	"description": "A web application for testing bookstore applications with fake book data generation",
	"main": "server/index.js",
	"scripts": {
		"start": "concurrently \"npm run server\" \"npm run client\"",
		"server": "cd server && npm start",
		"client": "cd client && npm start",
		"build": "cd client && npm run build",
		"install-all": "npm install && cd server && npm install && cd ../client && npm install"
	},
	"keywords": [
		"bookstore",
		"testing",
		"fake-data",
		"react",
		"node"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"seedrandom": "^3.0.5",
		"csv-writer": "^1.6.0"
	},
	"devDependencies": {
		"concurrently": "^8.2.2"
	}
}
